      64              ;//
      65              ;// Note that the watchdog is disabled in codestartbranch.asm
      66              ;// for this project. This is to prevent it from expiring while
      67              ;// c_init routine initializes the global variables before
      68              ;// reaching the main()
      69              ;//
      70              
      71        0001  WD_DISABLE  .set  1    ;set to 1 to disable WD, else set to 0
      72              
      73                  .ref _c_int00
      74                  .global code_start
      75              
      76              ***********************************************************************
      77              * Function: codestart section
      78              *
      79              * Description: Branch to code starting point
      80              ***********************************************************************
      81              
      82 00000000           .sect "codestart"
      83              
      84              code_start:
      85                  .if WD_DISABLE == 1
      86 00000000 0040!         LB wd_disable       ;Branch to watchdog disable code
         00000001 0000 
      87                  .else
      88                      LB _c_int00         ;Branch to start of boot._asm in RTS library
      89                  .endif
      90              
      91              ;end codestart section
      92              
      93              ***********************************************************************
      94              * Function: wd_disable
      95              *
      96              * Description: Disables the watchdog timer
      97              ***********************************************************************
      98                  .if WD_DISABLE == 1
      99              
     100 00000000           .text
     101 00000000       wd_disable:
     102 00000000 561F      SETC OBJMODE        ;Set OBJMODE for 28x object code
     103 00000001 7622      EALLOW              ;Enable EALLOW protected register access
     104 00000002 B9C0      MOVZ DP, #7029h>>6  ;Set data page for WDCR register
     105 00000003 2829      MOV @7029h, #0068h  ;Set WDDIS bit in WDCR to disable WD
         00000004 0068 
     106 00000005 761A      EDIS                ;Disable EALLOW protected register access
     107 00000006 0040!     LB _c_int00         ;Branch to start of boot._asm in RTS library
         00000007 0000 
